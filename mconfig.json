    "dest": "./www/",
    "build": "./build/",
    "styles": {
        "src": "./assets/styles/",
        "dest": "./www/assets/styles/",
        "main": "main"
    },
    "scripts": {
        "src": "./assets/scripts/",
        "dest": "./www/assets/scripts/",
